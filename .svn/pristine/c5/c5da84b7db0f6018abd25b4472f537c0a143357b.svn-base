.bankCard {
    width: 100%;
    height: 100%;
    .info_title {
        width: 858px;
        height: 50px;
        border-bottom: 1px solid #CCCCCC;
        margin: 40px 0 40px 40px;
        position: relative;
        p {
            font-family: MicrosoftYaHei-Bold;
            font-size: 24px;
            color: #333333;
            text-align: left;
            font-weight: 600;
        }
        em {
            font-family: MicrosoftYaHei;
            font-size: 18px;
            color: #FF5757;
            letter-spacing: 0;
            font-style: normal;
            position: absolute;
            right: 200px;
            top: 10px;
        }
        .bc_titbut {
            width: 160px;
            height: 40px;
            border: 1px solid #2E86D1;
            position: absolute;
            right: 0;
            top: 0;
            font-family: MicrosoftYaHei;
            font-size: 18px;
            color: #2E86D1;
            letter-spacing: 0;
            border-radius: 0;
        }
    }
    .add_wrap {
        width: 400px;
        height: 500px;
        margin: 0 auto;
        margin-top: 40px;
        text-align: center;
        position: relative;
        .input1,
        .input2,
        .input3,
        .input4 {
            width: 260px;
            height: 40px;
            position: absolute;
            right: 0;
            top: 0;
        }
        .input2 {
            top: 84px;
            padding-left: 20px;
            border: 1px solid #757575;
        }
        .input3 {
            top: 168px;
        }
        .input4 {
            top: 252px;
            padding-left: 20px;
            border: 1px solid #757575;
        }
        .input1,
        .input3 {
            right: 24px;
            .el-button {
                width: 284px;
                height: 100%;
                background: white;
                border-radius: 0;
                text-align: left;
                padding-left: 20px;
                color: #CCCCCC;
                border: 1px solid #757575;
                .el-icon--right {
                    position: absolute;
                    right: 10px;
                }
            }
            .el-dropdown-menu {
                width: 160px !important;
                position: relative;
                left: -122px;
            }
            .el-input__inner {
                width: 284px;
                border-radius: 0;
                color: #757575;
                border: 1px solid #757575;
            }
        }
        .icon1 {
            position: absolute;
            top: 261px;
            right: -29px;
            font-size: 18px;
        }
        .p1,
        .p2,
        .p3,
        .p4 {
            font-family: MicrosoftYaHei;
            font-size: 18px;
            color: #000000;
            letter-spacing: 0;
            position: absolute;
            top: 10px;
            left: 0;
        }
        .p2 {
            top: 95px;
        }
        .p3 {
            top: 180px;
        }
        .p4 {
            top: 264px;
        }
        .btn {
            width: 300px;
            height: 40px;
            position: absolute;
            left: 50px;
            bottom: 60px;
            border-radius: 0;
        }
        input::-ms-input-placeholder {
            font-family: MicrosoftYaHei;
            font-size: 14px;
            color: #CCCCCC;
            letter-spacing: 0;
        }
        input::-moz-input-placeholder {
            font-family: MicrosoftYaHei;
            font-size: 14px;
            color: #CCCCCC;
            letter-spacing: 0;
        }
        input::-o-input-placeholder {
            font-family: MicrosoftYaHei;
            font-size: 14px;
            color: #CCCCCC;
            letter-spacing: 0;
        }
        input::-webkit-input-placeholder {
            font-family: MicrosoftYaHei;
            font-size: 14px;
            color: #CCCCCC;
            letter-spacing: 0;
        }
    }
    .list_wrap {
        width: 858px;
        height: 160px;
        margin-left: 40px;
        text-align: center;
        .noData {
            margin-top: 200px;
            p {
                font-family: PingFangSC-Regular;
                font-size: 18px;
                color: #999999;
                letter-spacing: 0;
                padding-top: 15px;
            }
        }
        &>div {
            float: left;
            width: 230px;
            height: 120px;
            background: #FFFFFF;
            -webkit-box-shadow: 0 2px 8px 0 #CCCCCC;
            box-shadow: 0 2px 8px 0 #CCCCCC;
            margin-right: 45px;
            margin-bottom: 45px;
            text-align: center;
            position: relative;
            img {
                width: 26px;
                height: 26px;
                position: absolute;
                top: 20px;
                left: 10px;
            }
            .p1 {
                font-family: MicrosoftYaHei;
                font-size: 18px;
                color: #333333;
                letter-spacing: 0;
                padding-top: 21px
            }
            .p2 {
                font-family: PingFangSC-Regular;
                font-size: 14px;
                color: #333333;
                letter-spacing: 0;
                padding-top: 31px
            }
            .del {
                position: absolute;
                top: 0;
                right: 5px;
                font-size: 18px;
                cursor: pointer;
                color: #333333;
            }
        }
    }
}