.pay_wrap {
    width: 100%;
    min-height: 850px;
    .pay {
        width: 600px;
        height: 300px;
        margin: 0 auto;
        margin-top: 200px;
        text-align: center;
        .pay_in {
            width: 370px;
            height: 62px;
            margin: 0 auto;
            &>div {
                width: 60px;
                height: 60px;
                float: left;
                border: 1px solid #CCCCCC;
                text-align: center;
                border-right: unset;
                span {
                    display: inline-block;
                    width: 12px;
                    height: 12px;
                    margin-top: 24px;
                    background: black;
                    border-radius: 50%;
                }
            }
            &>div:last-child {
                border-right: 1px solid #CCCCCC;
            }
        }
        .but2 {
            width: 300px;
            height: 40px;
            border-radius: 0;
        }
    }
}